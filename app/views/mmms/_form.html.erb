<%= form_for(@mmm, :html => { :class => "form-horizontal" }) do |f| %>
  <% if @mmm.errors.any? %>
    <div class="alert alert-danger alert-dismissable">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <h4><%= pluralize(@mmm.errors.count, "error") %> prohibited this mmm from being saved:</h4>

      <ul>
      <% @mmm.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :mdu, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :mdu, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :tkd, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.number_field :tkd, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :adress, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :adress, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :porch, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :porch, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :ip, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :ip, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :sys_name, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :sys_name, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :ingress, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :ingress, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :mod, :class => "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.check_box :mod, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit :class => "btn btn-primary" %>
    </div>
  </div>
<% end %>
