<div class="page-header">
  <%= link_to avrs_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_avr_path(@avr), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show avr</h1>
</div>

<dl class="dl-horizontal">
  <dt>Mmm:</dt>
  <dd><%= @avr.mmm_id %></dd>

  <dt>Type avr:</dt>
  <dd><%= @avr.type_avr %></dd>

  <dt>Material:</dt>
  <dd><%= @avr.material %></dd>

  <dt>User:</dt>
  <dd><%= @avr.user_id %></dd>

  <dt>Date on:</dt>
  <dd><%= @avr.date_on %></dd>

  <dt>Date off:</dt>
  <dd><%= @avr.date_off %></dd>

</dl>
